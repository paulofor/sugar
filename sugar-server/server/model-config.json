{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Projeto": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "Experiencia": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "ItemCusto": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "deleteAll": false,
          "deleteById": false
        }
      }
    }
  },
  "PerguntaFormulario": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "Baby": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "RespostaFormulario": {
    "dataSource": "mysqlDS",
    "public": true
  },
  "TracoPersonalidade": {
    "dataSource": "mysqlDS",
    "public": true
  }
}
